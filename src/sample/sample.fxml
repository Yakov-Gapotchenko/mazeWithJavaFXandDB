<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


   <VBox spacing="10.0" fillWidth="true" alignment="center" minHeight="600" minWidth="500" maxHeight="600" maxWidth="500"  xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">

   <children>
      <Label wrapText="true"  minHeight="100" maxHeight="100" textAlignment="CENTER"
              text="Maze: S - start(the initial position of the maze) , F - finish(end position of the maze) , * - wall, 0 - free cell " />
      <TextArea minHeight="235" maxHeight="235" fx:id="mazeInputTextArea" />
      <Button mnemonicParsing="false" text="Find a way: " minHeight="30" maxHeight="30" textAlignment="CENTER" fx:id="btnFindWay" onAction="#onBtnFindWay" />
      <Label text="Here will be the result..." minHeight="235" maxHeight="235" textAlignment="CENTER" fx:id="mazeResOutputLabel" />
   </children>
      <padding>
         <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
      </padding>
   </VBox>


